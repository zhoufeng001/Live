<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zf.live.dao.mapper.VideoDetailMapperExt" >
  <!--
  用于扩展com.zf.live.dao.mapper.VideoDetailMapper接口
可通过com.zf.live.dao.mapper.VideoDetailMapper.BaseResultMap 
和com.zf.live.dao.mapper.VideoDetailMapper.Base_Column_List对其进行引用
by is_zhoufeng@163.com 2015-01-05 00:14:43.
  -->
  
  <select id="countVideoDetail" resultType="java.lang.Integer">
  		 select 
  		 count(*)
	     from video_detail
	     where
	     id = (select id from video where videofrom = #{videofrom} and fromid = #{fromId})
	     limit 1
  </select>
  
  
  <select id="selectVideoDetailId" resultType="java.lang.Long">
 		 select 
 		 id
	     from video_detail
	     where
	     id = (select id from video where videofrom = #{videofrom} and fromid = #{fromId})
	     limit 1
  </select>
  
</mapper>